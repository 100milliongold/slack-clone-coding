{
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "markdown",
    "typescript",
    "typescriptreact"
  ],
  "prettier.formatterPriority": -1,
  "styled-components.tags": ["styled", "css", "sty", "createGlobalStyle"],
  "prettier.configPath": "~/git/slack-clone-coding/prettier.json",
  "extensions": {
    "coc-tsserver": "1.11.11",
    "coc-prettier": "9.3.1",
    "coc-eslint": "1.5.8",
    "coc-marketplace": "1.9.0",
    "coc-styled-components": "0.5.0",
    "coc-react-refactor": "0.5.0",
    "coc-docker": "0.5.0",
    "coc-java": "1.5.5"
  },
  "java.configuration.runtimes": [
    {
      "name": "AdoptOpenJDK 11",
      "path": "/Library/Java/JavaVirtualMachines/adoptopenjdk-11.jdk/Contents/Home/bin/java",
      "default": true
    },
    {
      "name": "Zulu 8.58.0.13",
      "path": "/Library/Java/JavaVirtualMachines/zulu-8.jdk/Contents/Home/bin/java"
    }
  ]
}
